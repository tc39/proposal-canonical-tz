{
  "name": "proposal-canonical-tz",
  "version": "0.1.0",
  "description": "Non-production polyfill (stacked on Temporal) for the TC39 Time Zone Canonicalization proposal",
  "type": "commonjs",
  "main": "dist/index.js",
  "browser": "dist/index.umd.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "node ./test/all.mjs",
    "test262": "npm run build262 && TIMEOUT=30000 node runtest262.mjs",
    "build": "rollup -c rollup.config.js --bundleConfigAsCjs",
    "build262": "TEST262=1 rollup -c rollup.config.js --bundleConfigAsCjs",
    "prepublishOnly": "npm run build",
    "playground": "node -r ./lib/init.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tc39/proposal-canonical-tz.git",
    "directory": "polyfill"
  },
  "author": "Justin Grant <justingrant@justingrant@users.noreply.github.com>",
  "contributors": [
    "Justin Grant",
    "Richard Gibson"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tc39/proposal-canonical-tz/issues"
  },
  "homepage": "https://github.com/tc39/proposal-canonical-tz/",
  "files": [
    "index.d.ts",
    "dist",
    "lib"
  ],
  "dependencies": {
    "big-integer": "^1.6.51",
    "call-bind": "^1.0.2",
    "es-abstract": "^1.21.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@js-temporal/temporal-test262-runner": "^0.9.0",
    "@pipobscure/demitasse": "^1.0.10",
    "@pipobscure/demitasse-pretty": "^1.0.10",
    "@pipobscure/demitasse-run": "^1.0.10",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-replace": "^5.0.2",
    "c8": "^7.13.0",
    "codecov": "^3.8.2",
    "rollup": "^3.21.5",
    "rollup-plugin-node-polyfills": "^0.2.1"
  },
  "engines": {
    "node": ">=18"
  }
}
